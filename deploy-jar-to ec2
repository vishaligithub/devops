node{
   agent any

   tools {
      // Install the Maven version configured as "M3" and add it to the path.
      maven 'maven'
   }

   stages {
     
      stage('Deploy to Tomcat'){
      
         sshagent(['ssh_ec2']) {
         sh 'scp -o StrictHostKeyChecking=no /var/lib/jenkins/workspace/pipeline/target/*.war ubuntu@3.15.212.160:/opt'
        }
        
      }
    }
  
}
